{
  "name": "vuegular",
  "version": "0.0.1",
  "private": true,
  "description": "Having fun with Vuejs!",
  "repository": "https://github.com/ludanxer/vuegular.git",
  "license": "MIT",
  "author": "ludanxer <ludanxer@gmail.com>",
  "scripts": {
    "build": "webpack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "sort-package-json": "^1.40.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
